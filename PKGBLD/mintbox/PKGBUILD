pkgname=mintbox
pkgver=0.11
pkgrel=1
pkgdesc="Small wrapper around nud walletpassphrase"
provides=('mintbox')
makedepends=('python2')
depends=('python2-sh' 'python2-colored')
arch=('any')
license=('GPL')
source=( "${pkgname}.py" )

sha256sums=('c4a3d1ad20cbbe13451c0ad1279269477faf28e1b892fb8e88b2c987077bf1b2')

package() {
install -Dm755 ${pkgname}.py ${pkgdir}/usr/bin/${pkgname}
}
